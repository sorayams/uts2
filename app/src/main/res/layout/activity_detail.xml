<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".detail">

    <ImageView
            android:layout_width="254dp"
            android:layout_height="127dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="88dp" app:layout_constraintHorizontal_bias="0.496"/>
    <TextView
            android:text="Nama, umur"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/idNama"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.236" app:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="32dp"/>
    <TextView
            android:text="Kelamin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/idKelamin"
            app:layout_constraintTop_toBottomOf="@+id/idNama" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.219"/>
    <TextView
            android:text="Ras"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/idRas"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.201" tools:layout_editor_absoluteY="302dp"/>
    <TextView
            android:text="Keterangan"
            android:layout_width="254dp"
            android:layout_height="216dp" android:id="@+id/idKeterangan"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintTop_toBottomOf="@+id/idKelamin" android:layout_marginTop="60dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>